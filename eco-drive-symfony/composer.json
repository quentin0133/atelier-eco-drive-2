{
    "require": {
        "symfony/skeleton": "^8.0",
        "symfony/console": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "mongodb/mongodb": "^2.2",
        "symfony/framework-bundle": "^8.0",
        "symfony/yaml": "^8.0",
        "symfony/runtime": "^8.0",
        "symfony/dotenv": "^8.0",
        "doctrine/mongodb-odm-bundle": "^5.6",
        "symfony/routing": "^8.0",
        "symfony/http-kernel": "^8.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "symfony/flex": "^2.10",
        "symfony/debug-bundle": "^8.0"
    },
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
